Package: oce
Type: Package
Title: Analysis of Oceanographic Data
Version: 1.0-2.1
Authors@R: c(
    person(given="Dan", family="Kelley", email="Dan.Kelley@Dal.Ca", role=c("aut", "cre"), comment=c(ORCID="https://orcid.org/0000-0001-7808-5911")),
